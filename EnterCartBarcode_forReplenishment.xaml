<Window x:Class="PTLStation.EnterCartBarcode_forReplenishment"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PTLStation"
        mc:Ignorable="d"
        Title="Введите штрих-код корзины" Height="150" Width="450" ResizeMode="NoResize" Icon="/icon.png">
    <Grid Background="#FFA3A3A3">
        <TextBox x:Name="textBox" Margin="10" TextWrapping="Wrap" VerticalAlignment="Center" TextChanged="textBox_TextChanged" BorderBrush="{x:Null}" FontSize="20"/>

    </Grid>
</Window>
